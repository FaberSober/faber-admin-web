{
  "name": "react-simple",
  "version": "1.0.0",
  "main": "main.js",
  "repository": "",
  "description": "react-simple",
  "scripts": {
    "start": "npm run vite:dev",
    "webpack:dev": "cross-env NODE_ENV=development webpack serve --mode development",
    "vite:dev": "vite --port=4002",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "serve": "serve dist",
    "test": "npm run build && npm run serve",
    "lint": "eslint --fix \"src/**/*.{js,ts,jsx,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,ts,jsx,tsx}\" ./*.{js,ts}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
